{
    "name": "dansoppelsa/carwash",
    "description": "Data scrubber for Laravel applications",
    "type": "library",
    "require": {
        "fzaninotto/faker": "^1.7",
        "illuminate/database": "~5.5",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.0",
        "orchestra/testbench" : "~3.5",
        "orchestra/database": "3.5.*"
    },
    "autoload": {
        "psr-4" : {
            "Carwash\\" : "src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Carwash\\" : "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Soppelsa",
            "email": "dsoppelsa@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Carwash\\CarwashProvider"
            ]
        }
    }
}
